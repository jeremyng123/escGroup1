
doctype html
html(lang='en')
  include common/header.pug
  title=title
  +header(title)
  body
  head
    
    script(async src='https://www.googletagmanager.com/gtag/js?id=UA-138699201-1')
    script.
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'UA-138699201-1');
    include common/navbar.pug
    +navbar(user)
    .container-fixed
      html
  head
  body
    nav.navbar.navbar-inverse.navbar-fixed-top
        #navbar.navbar-collapse.collapse
          ul.nav.navbar-nav.navbar-right
            li
              a(href='#') Quick Access
            li
              a(href='#') Settings
            li
              a(href='#') Profile
            li
              a(href='#') Help
